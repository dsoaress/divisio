    "fastify": "5.2.1",
    "ioredis": "5.5.0",
    "pg": "8.13.3",
    "zod": "3.24.2",
    "zod-validation-error": "3.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "9.5.0",
    "@testcontainers/redis": "10.18.0",
    "@types/node": "22.13.4",
    "@types/pg": "8.11.11",
    "@types/supertest": "6.0.2",
    "drizzle-kit": "0.30.4",
    "pino-pretty": "13.0.0",
    "supertest": "7.0.0",
    "tsup": "8.3.6",
    "tsx": "4.19.3",
    "typescript": "5.7.3"
